{
  "presets": ["es2015", "react"],
  "plugins": [
    'transform-runtime',
    'transform-decorators-legacy',
    'transform-class-properties',
    'react-hot-loader/babel',
    [
      "module-resolver", {
        "root": ["./src"],
        "alias": {
          "assets": "./assets",
          "styles": "./styles",
          "components": "./components",
        }
      }
    ],
  ]
}
